#!/usr/bin/perl

use strict;
use warnings;
use 5.010;
use NX::myprompt;

# ABSTRACT: Custom tcsh prompt for me
# PODNAME: myprompt
# VERSION

=head1 SYNOPSIS

 % myprompt --cshrc
 % myprompt --shrc
 % myprompt

=head1 OPTIONS

=head2 --cshrc

Print out the appropriate .cshrc configurations.

=head2 --shrc

Print out the appropriate .shrc configurations.

=head2 --help

Print help and exit.

=head2 --version

Print version and exit.

=cut

exit NX::myprompt->main(@ARGV);
